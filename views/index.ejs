<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv="refresh" content="2"  >
	</head>
	<body>
		<p><%= h %></p>
		<table style="width:100%"  border='1'>
			<tr>
					<td>溫度</td>
					<td>紅外線</td>
					<td>粉塵</td>
					<td>溼度</td>
					<td>瓦斯</td>
				</tr>
			<% data.forEach(function(d){ %>
				<tr>
					<td><%= d.Tmp %></td>
					<td><%= d.PIR %></td>
					<td><%= d.Air %></td>
					<td><%= d.Hum %></td>
					<td><%= d.Gas %></td>
				</tr>
			<% }); %>
		</table>
		<table style="width:50%"  border='1'>
			<tr>
					<td>RFID</td>
					<td>time</td>
				</tr>
			<% rfid.forEach(function(r){ %>
				<tr>
					<td><%= r.RFID %></td>
					<td><%= r.date %></td>
				</tr>
			<% }); %>
		</table>
	</body>
</html> 

